{
  "name": "supercoin",
  "version": "0.1.0",
  "description": "Unified AI CLI hub for Claude, Codex, and Gemini",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "supercoin": "dist/cli/index.js"
  },
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun run scripts/dev.ts",
    "dev:setup": "bun run scripts/dev-setup.ts",
    "start": "bun src/cli/index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "tsc --noEmit",
    "typecheck": "bun run scripts/build.ts && tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.9",
    "@ai-sdk/google": "^3.0.6",
    "@ai-sdk/openai": "^3.0.7",
    "@clack/prompts": "^0.7.0",
    "ai": "^6.0.26",
    "commander": "^12.0.0",
    "hono": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "cli",
    "ai",
    "claude",
    "codex",
    "gemini",
    "supercoin"
  ],
  "author": "supercent",
  "license": "MIT"
}
