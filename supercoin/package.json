{
  "name": "supercoin",
  "version": "0.1.0",
  "description": "Unified AI CLI hub for Claude, Codex, and Gemini",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "supercoin": "dist/cli/index.js"
  },
  "scripts": {
    "dev": "bun --hot src/cli/index.ts",
    "build": "bun build src/cli/index.ts --outdir dist/cli --target node",
    "start": "bun src/cli/index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "commander": "^12.0.0",
    "hono": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "cli",
    "ai",
    "claude",
    "codex",
    "gemini",
    "supercoin"
  ],
  "author": "supercent",
  "license": "MIT"
}
