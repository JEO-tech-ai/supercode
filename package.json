  "private": true,
  "description": "Unified AI CLI hub for Claude, Codex, Gemini, and localhost models",
  "type": "module",
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "packages/*",
    "packages/config/*",
    "packages/console/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JEO-tech-ai/supercoin.git"
  },
  "bugs": {
    "url": "https://github.com/JEO-tech-ai/supercoin/issues"
